! This configuration is supposed to match the rrfs_dev1 parallel, so
! do not change the namelist without checking the current parallel.

&atmos_model_nml
    blocksize = 32
    ccpp_suite = '@[CCPP_SUITE]'
    chksum_debug = .false.
    dycore_only = .false.
/

&cires_ugwp_nml
    knob_ugwp_azdir = 2, 4, 4, 4
    knob_ugwp_doaxyz = 1
    knob_ugwp_doheat = 1
    knob_ugwp_dokdis = 1
    knob_ugwp_effac = 1, 1, 1, 1
    knob_ugwp_ndx4lh = 1
    knob_ugwp_solver = 2
    knob_ugwp_source = 1, 1, 0, 0
    knob_ugwp_stoch = 0, 0, 0, 0
    knob_ugwp_version = 0
    knob_ugwp_wvspec = 1, 25, 25, 25
    launch_level = 25
/

&diag_manager_nml
    prepend_date = .false.
    max_output_fields = @[MAX_OUTPUT_FIELDS]
/

&external_ic_nml
    checker_tr = .false.
    filtered_terrain = .true.
    gfs_dwinds = .true.
    levp = 66
    nt_checker = 0
/

&fms_io_nml
    checksum_required = .false.
    max_files_r = 100
    max_files_w = 100
/

&fms_nml
    clock_grain = 'ROUTINE'
    domains_stack_size = 5000000 ! Smoke needs a higher value than 3000000 default.
    print_memory_usage = .false.
/

