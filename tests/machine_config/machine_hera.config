#!/bin/bash
set -eux

if [[ "${ROCOTO:-false}" == true ]] ; then
    module load rocoto
    ROCOTORUN=$(which rocotorun)
    ROCOTOSTAT=$(which rocotostat)
    ROCOTOCOMPLETE=$(which rocotocomplete)
    ROCOTO_SCHEDULER=slurm
fi

